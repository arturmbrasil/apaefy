<p id="notice"><%= notice %></p>

<h1>Colaboradors</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nome</th>
      <th>Data nascimento</th>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Telefone</th>
      <th>Sexo</th>
      <th>Cnpj</th>
      <th>Inscricao estadual</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colaboradors.each do |colaborador| %>
      <tr>
        <td><%= colaborador.codigo %></td>
        <td><%= colaborador.nome %></td>
        <td><%= colaborador.data_nascimento %></td>
        <td><%= colaborador.rg %></td>
        <td><%= colaborador.cpf %></td>
        <td><%= colaborador.telefone %></td>
        <td><%= colaborador.sexo %></td>
        <td><%= colaborador.cnpj %></td>
        <td><%= colaborador.inscricao_estadual %></td>
        <td><%= link_to 'Show', colaborador %></td>
        <td><%= link_to 'Edit', edit_colaborador_path(colaborador) %></td>
        <td><%= link_to 'Destroy', colaborador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Colaborador', new_colaborador_path %>
