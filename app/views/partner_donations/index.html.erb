<p id="notice"><%= notice %></p>

<h1>Partner Donations</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Payment type</th>
      <th>Item</th>
      <th>Partner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_donations.each do |partner_donation| %>
      <tr>
        <td><%= partner_donation.value %></td>
        <td><%= partner_donation.payment_type %></td>
        <td><%= partner_donation.item %></td>
        <td><%= partner_donation.partner %></td>
        <td><%= link_to 'Show', partner_donation %></td>
        <td><%= link_to 'Edit', edit_partner_donation_path(partner_donation) %></td>
        <td><%= link_to 'Destroy', partner_donation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner Donation', new_partner_donation_path %>
