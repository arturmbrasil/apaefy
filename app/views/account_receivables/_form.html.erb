          <!--<%= f.collection_select(:partner_donations_id, PartnerDonation.all, :id, :acronym, { prompt: true }, { class: 'form-control js-state-select' }) %>-->
        <!--</div>-->

        <div class="form-group">
          <%= f.submit 'Enviar', class: 'btn btn-primary' %>
        </div>
