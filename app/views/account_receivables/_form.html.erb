<div class="card">
  <div class="card-body">
    <%= form_with(model: account_receivable, local: true) do |f| %>
        <%= errors_for(account_receivable) %>

        <div class="form-group">
          <%= f.label :name, class: 'form-control-label' %>
          <%= f.text_field :name, class: 'form-control', required: true %>
        </div>

        <div class="form-group">
          <%= f.label :date, class: 'form-control-label' %>
          <%= f.date_field :date, class: 'form-control', required: true %>
        </div>

        <div class="form-group">
          <%= f.label :value, class: 'form-control-label' %>
          <%= f.text_field :value, class: 'form-control', required: true %>
        </div>

        <!--<div class="form-group">-->
