wb = xlsx_package.workbook
statusProj = {'A' => 'Andamento', 'F' => 'Finalizado'}
wb.add_worksheet(name: 'plan1') do |sheet|
  sheet.add_row ['Título', 'Descrição', 'Metas', 'Status', 'Valor']
  @projetos_exp.each do |row|
    sheet.add_row [row.titulo, row.descricao, row.metas, statusProj[row.status], row.valor]
  end
end